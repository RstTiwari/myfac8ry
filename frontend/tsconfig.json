{
  "compilerOptions": {
    "jsx": "react-jsx", // or "react" if you're using an older React version
    "target": "es5", // or another target based on your project
    "module": "esnext",
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node"],
    
  },
  "include": [
    "src/**/*",  // Make sure to include all necessary directories
    "src/**/*.jsx"  // Include .jsx files if not already in the pattern
  ]
}
